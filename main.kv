#:import SwapTransition kivy.uix.screenmanager.SwapTransition
#:import SlideTransition kivy.uix.screenmanager.SlideTransition
#:import Window kivy.core.window.Window
#:import show_snackbar tarefas.show_snackbar
Principal:
    name: "main"

    MDBottomNavigation:
        text_color_normal: "#abbdf2"
        text_color_active: 0, 0, 0, 1
        #panel_color: "#eeeaea"
        selected_color_background: 0, 0, 0, 0

        MDBottomNavigationItem:
            font_name: 'Kumbh'
            id: nav
            name: 'screen 1'
            text: 'Home'
            icon: 'home'
            MDBoxLayout:
                orientation: "vertical"
                MDTabs:
                    id: tabs
                    Tab:
                        font_name: 'Kumbh'
                        title: "Tarefas disponíveis"
                        icon: "format-list-numbered"
                        MDScrollView:
                            MDBoxLayout:
                                id: tasks
                                padding: 10
                                spacing: 10
                                size_hint_y: None
                                orientation: "vertical"
                                height: self.minimum_height
                                on_minimum_height: self.height = self.minimum_height
                    
                        MDBoxLayout:
                            id: box
                            spacing: "56dp"
                            adaptive_size: True
                            pos_hint: {"center_x": .83, "center_y": .10}
                            MDIconButton:
                                icon: "plus-circle"
                                icon_size: "40sp"
                                on_press:
                                    root.manager.transition = SwapTransition()
                                    root.manager.current = "tasks_send"
                                    app.screen_manager.transition = SlideTransition()
                    Tab:
                        font_name: 'Kumbh'
                        title: "Iniciadas"
                        icon:"clock-check"
                        MDScrollView:
                            MDBoxLayout:
                                id: tasks_ongoing
                                padding: 10
                                spacing: 10
                                size_hint_y: None
                                orientation: "vertical"
                                height: self.minimum_height
                                on_minimum_height: self.height = self.minimum_height
                    Tab:
                        font_name: 'Kumbh'
                        title: "Finalizadas"
                        icon: "check-all"
                        MDScrollView:
                            MDBoxLayout:
                                id: tasks_finished
                                padding: 10
                                spacing: 10
                                size_hint_y: None
                                orientation: "vertical"
                                height: self.minimum_height
                                on_minimum_height: self.height = self.minimum_height

                

        MDBottomNavigationItem:
            font_name: 'Kumbh'
            name: 'screen 2'
            text: 'Busca'
            icon: 'book-search'

            ScreenListItems
        MDBottomNavigationItem:
            font_name: 'Kumbh'
            name: 'screen 3'
            text: 'Diário'
            icon: "book-information-variant"
            MDBoxLayout:
                orientation: "vertical"
                MDTabs:
                    id: tabs
                    Tab:
                        font_name: 'Kumbh'
                        title: "PRECIFICAÇÃO"
                        icon: "printer-check"
                        MDBoxLayout:
                            orientation: "vertical"
                            MDTopAppBar:
                                id: toolbar
                                title: "Itens a precificar"
                                left_action_items: [["menu"]]
                                right_action_items: [["dots-vertical"]]
                                md_bg_color: 0, 0, 0, 1
                            ScrollView:
                                #scroll_timeout : 100
                                MDSelectionList:
                                    id: md_list
                                    spacing: "12dp"
                                    overlay_color: app.overlay_color[:-1] + [.2]
                                    icon_bg_color: app.overlay_color
                                    on_selected: app.on_selected(*args)
                                    on_unselected: app.on_unselected(*args)
                                    on_selected_mode: app.set_selection_mode(*args)
                        MDBoxLayout:
                            orientation: "vertical"
                            size_hint_y: None
                            height: "0dp"
                            padding: "5dp"
                            pos_hint: {"center_x": 1.25, "center_y": 0}
                            MDIconButton:
                                icon: "plus-circle"
                                icon_size: "40sp"
                                on_press:
                                    root.manager.transition = SwapTransition()
                                    root.manager.current = "prices_add"
                                    app.screen_manager.transition = SlideTransition()
                        MDBoxLayout:
                            orientation: "vertical"
                            size_hint_y: None
                            height: "0dp"
                            padding: "5dp"
                            pos_hint: {"center_x": 1, "center_y": 0}
                            MDIconButton:
                                icon: "refresh-circle"
                                icon_size: "40sp"
                                on_press: if app.add_all_items_from_firebase(): show_snackbar("Atualizado!")
                    Tab:
                        font_name: 'Kumbh'
                        title: "ABASTECIMENTO"
                        icon: "pickaxe"
                        MDScrollView:
                            scroll_timeout : 100
                            MDList:
                                id: lista_abastecimento
                                padding: 0
                        MDBoxLayout:
                            orientation: "vertical"
                            size_hint_y: None
                            height: "0dp"
                            padding: "5dp"
                            pos_hint: {"center_x": 1, "center_y": 0}
                            MDIconButton:
                                icon: "refresh-circle"
                                icon_size: "40sp"
                                on_press: app.add_abastecimento_firebase()
                    Tab:
                        font_name: 'Kumbh'
                        title: "VALIDADE"
                        icon: 'calendar-end'
                        FloatLayout:
                            MDFillRoundFlatButton:
                                font_name: 'Kumbh'
                                id: botao_ver_lista
                                text: "VER LISTA"
                                size_hint_x:"0.8"
                                pos_hint: {'center_x': 0.5, 'center_y': 0.83}
                                on_press:
                                    root.manager.transition = SwapTransition()
                                    root.manager.current = "tela_validade"
                                    app.screen_manager.transition = SlideTransition()
                            MDFillRoundFlatButton:
                                font_name: 'Kumbh'
                                id: botao_fazer_validade
                                text: "FAZER VALIDADE"
                                size_hint_x:"0.8"
                                pos_hint: {'center_x': 0.5, 'center_y': 0.73}
                                on_press:
                                    root.manager.transition = SwapTransition()
                                    root.manager.current = "fazer_validade"
                                    app.screen_manager.transition = SlideTransition()
        MDBottomNavigationItem:
            font_name: 'Kumbh'
            name: 'screen 4'
            text: 'Perfil'
            icon: "account-box"
            MDBoxLayout:
                orientation: "vertical"
                MDTabs:
                    id: tabs
                    Tab:
                        font_name: 'Kumbh'
                        title: "Meus dados"
                        icon: "account-box"
                        FloatLayout:
                
                            CampoDados:
                                font_size: "18sp"
                                mode: "rectangle"
                                text: "teste"
                                id: name
                                hint_text: "nome completo"
                                icon_right: "account"
                                pos_hint: {'center_x': 0.5, 'center_y': 0.90}
                            CampoDados:
                                font_size: "18sp"
                                mode: "rectangle"
                                id: birth
                                hint_text: "nascimento dd/mm/yy"
                                icon_right: "calendar"
                                pos_hint: {'center_x': 0.5, 'center_y': 0.77}
                            CampoDados:
                                font_size: "18sp"
                                mode: "rectangle"
                                id: pnum
                                hint_text: "celular"
                                icon_right: "cellphone"
                                pos_hint: {'center_x': 0.5, 'center_y': 0.64}
                            CampoDados:
                                font_size: "18sp"
                                mode: "rectangle"
                                id: mail
                                hint_text: "e-mail"
                                icon_right: "mail"
                                pos_hint: {'center_x': 0.5, 'center_y': 0.51}                 
                            CampoDados:
                                font_size: "18sp"
                                mode: "rectangle"
                                id: username
                                hint_text: "nome de usuário"
                                icon_right: "account"
                                pos_hint: {'center_x': 0.5, 'center_y': 0.38}
                            CampoDados:
                                font_size: "18sp"
                                mode: "rectangle"
                                id: setor
                                hint_text: "Setor"
                                icon_right: "account"
                                pos_hint: {'center_x': 0.5, 'center_y': 0.25}
                            MDRectangleFlatIconButton:
                                font_name: 'Kumbh'
                                font_size: "30sp"
                                line_color: 0, 0, 0, 0
                                text: "DESLOGAR"
                                icon: "logout"
                                line_color: 0, 0, 0, 0
                                pos_hint: {"center_x": .5, "center_y": .12}
                                on_press: 
                                    app.show_alert_dialog_logout()
                    Tab:
                        title: "Configurações"
                        icon: "account-settings"
                        font_name: 'Kumbh'
                        MDFloatLayout:
                            MDSwitch:
                                pos_hint: {'center_x': .5, 'center_y': .5}
                                icon_active: "check"
                                icon_active_color: "white"
<ScreenListItems>:
    name: "list_items"
    FloatLayout:
        CampoPesquisa:
            id: pesquisa 
            hint_text: 'Digite o que procura'
            text_validade_unfocus: False
            pos_hint: {'center_x': 0.5, 'center_y': 0.9}
            size_hint: .8, .11
            on_text: root.lista_a_procura(self.text, True)
        RecycleView:
            id: rv
            viewclass: 'ListaItemsComImg'
            key_size: 'height'
            pos_hint: {'top': 0.8}
            RecycleBoxLayout:
                padding: dp(10)
                default_size: None, dp(70)
                default_size_hint: 1, None
                size_hint_y: None
                height: self.minimum_height
                orientation: 'vertical'
<ListaItemsComImg>:
    font_name: 'Kumbh'
    text: "DUAS LINHAS COM AVATAR"
    secondary_text: "SEGUNDA LINHA AQUI"
    on_press: app.dialogo_adiciona_item_da_lista(self.text, 1, self.secondary_text)

<CampoPesquisa@MDTextField>:
    font_name: 'Kumbh'
    mode: "fill"
    font_size: '20sp'
    normal_color: app.theme_cls.primary_dark
    color_active: app.theme_cls.primary_dark
    halign: 'center'
    multiline: False
    icon: "clock-alert"

<Content>
    orientation: "vertical"
    spacing: "12dp"
    size_hint_y: None
    height: "120dp"

    MDTextField:
        hint_text: "Título"

    MDTextField:
        hint_text: "Descrição"

<CampoDados@MDTextField>:
    font_name: 'Kumbh'
    size_hint_x: "0.8"
    font_size: "18sp"

<MD3Card>
    line_color: 0.1, 0.9, 0.9, 0.8
    height: "180dp"
    padding: "10dp", "10dp"
    size_hint: 1, None
    style: "outlined" 
    radius: [10,10,10,10]

<MD4Card>
    line_color: 0.1, 0.9, 0.9, 0.8
    height: "250dp"
    padding: "10dp", "10dp"
    size_hint: 1, None
    style: "outlined" 
    radius: [10,10,10,10]
    _no_ripple_effect: True