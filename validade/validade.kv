#:import cria_arquivo_openoffice validade.cria_arquivo_openoffice
#:import show_snackbar tarefas.show_snackbar
Validade:
    name: 'tela_validade'
    MDBoxLayout:
        orientation: "vertical"
        MDTopAppBar:
            id: val_toolbar
            title: "Lista de validade"
            left_action_items: [["arrow-collapse-left", lambda x: app.go_to_main_screen()]]
            right_action_items: [["download", lambda x: (cria_arquivo_openoffice(root.data_tables), show_snackbar("Arquivo salvo em downloads"))]]
        AnchorLayout:
            id: lista_validade