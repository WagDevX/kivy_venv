#:import save_to_openoffice validade.save_to_openoffice
#:import show_snackbar tarefas.show_snackbar
Validade:
    name: 'tela_validade'
    MDBoxLayout:
        orientation: "vertical"
        MDTopAppBar:
            id: val_toolbar
            title: "Lista de validade"
            left_action_items: [["arrow-collapse-left", lambda x: app.go_to_main_screen()]]
            right_action_items: [["trash-can"],["download", lambda x: (save_to_openoffice(root.data_tables, root.num_col), show_snackbar("Arquivo salvo em downloads"))]] + root.right_action_items
        AnchorLayout:
            id: lista_validade
